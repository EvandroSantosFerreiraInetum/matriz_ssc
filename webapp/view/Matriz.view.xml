<mvc:View controllerName="inetum.ssm.alocationssc.controller.Matriz"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:f="sap.f"
    xmlns:tab="sap.ui.table"
    xmlns:core="sap.ui.core" >

    <f:DynamicPage id="idDynamicPage" preserveHeaderStateOnScroll="true"  showFooter="true" fitContent="true">

    	<!-- DynamicPage Title -->
        <f:title>
            <f:DynamicPageTitle >
                <f:heading>
                    <Title text="Matriz SSC"/>
                </f:heading>
 
                <f:content>
                    <OverflowToolbar>
 
                    </OverflowToolbar>
                </f:content>
                <f:actions>

                </f:actions>
            </f:DynamicPageTitle>
        </f:title>

        <!-- DynamicPage Header -->
		<f:header>          
		    <f:DynamicPageHeader id="idDynamicPageHeader"  >                
                <fb:FilterBar id="idFilterBar" persistencyKey="myPersKey" useToolbar="false" search=".onSearch" filterChange=".onFilterChange" afterVariantLoad=".onAfterVariantLoad">
                    <fb:filterGroupItems>
                        <fb:FilterGroupItem name="Year" label="Year" groupName="Group1" visibleInFilterBar="true">
                            <fb:control>
                                <ComboBox
                                    name="Year"
                                    selectionChange=".onSelectionChange"
                                    items="{
                                        path: '/Year_ListSet',
                                        templateShareable: true
                                    }"
                                    selectedKey="2024"
                                >
                                    <core:Item key="{Year}" text="{Year}"/>
                                </ComboBox>
                            </fb:control>
                        </fb:FilterGroupItem>                        
                        <fb:FilterGroupItem name="Consultant" label="Consultant" groupName="Group2" visibleInFilterBar="true">
                            <fb:control>
                                <MultiComboBox
                                    name="Consultant"
                                    selectionChange=".onSelectionChange"
                                    items="{
                                        path: '/Consultant_ListSet',
                                        templateShareable: true
                                    }"
                                >
                                    <core:Item key="{Consultant}" text="{ConsultantName}"/>
                                </MultiComboBox>
                            </fb:control>
                        </fb:FilterGroupItem>
                        <fb:FilterGroupItem name="Data Type" label="Data Type" groupName="Group2" visibleInFilterBar="true">
                            <fb:control>
                                <MultiComboBox
                                    name="Data Type"
                                    selectionChange=".onSelectionChange"
                                    items="{
                                        path: '/Data_Type_ListSet',
                                        templateShareable: true
                                    }"
                                >
                                    <core:Item key="{DataType}" text="{DataType}"/>
                                </MultiComboBox>
                            </fb:control>
                        </fb:FilterGroupItem>                        
                    </fb:filterGroupItems>    
                </fb:FilterBar>
            </f:DynamicPageHeader>
        </f:header>

        <f:content>                     
            <tab:Table id="idTable" rows="{ path: '/ConsultantMatrizSet', events: {dataReceived: '.onDataReceive'} }" threshold="100000000"  visibleRowCount="20"  enableColumnFreeze="true" alternateRowColors="true" selectionMode="None" >                                    
                <tab:columns>
                    <tab:Column id="idColumn" width="11rem" filterProperty="ConsultantName">
                        <Label text="Consultant" textAlign="Center" width="100%" design="Bold"/>
                        <tab:template>
                            <Text text="{ConsultantName}"/>
                        </tab:template>
                    </tab:Column>   
                    <tab:Column width="11rem" filterProperty="Field1" > 
                        <Label text="" textAlign="Center" width="100%" design="Bold"/>
                            <tab:template>
                                <Text text="{DataType}"/>
                        </tab:template>
                    </tab:Column>                                                      
                    <tab:Column width="11rem" filterProperty="Time1" headerSpan="2"  autoResizable="true" hAlign="Center" > 
                        <tab:multiLabels>
                            <Label text="January - 144H" textAlign="Center" width="100%" design="Bold"/>
                            <Label text=" "  />
                        </tab:multiLabels>
                        <tab:template>
                            <Text text="{TimeM1}H"/>
                        </tab:template>
                    </tab:Column>      
                    <tab:Column width="11rem" filterProperty="Percent1" headerSpan="2" autoResizable="true" hAlign="Center" >  
                        <tab:multiLabels>
                            <Label text="January" design="Bold"/>
                            <Label text=" " />
                        </tab:multiLabels>
                        <tab:template>
                            <Text text="{PercentM1}%"/>
                        </tab:template>
                    </tab:Column>  
                        <tab:Column width="11rem" filterProperty="Time2" headerSpan="2" autoResizable="true" hAlign="Center" > 
