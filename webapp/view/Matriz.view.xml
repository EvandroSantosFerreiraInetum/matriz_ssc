                                <Label text="February - 116H" design="Bold" textAlign="Center" width="100%"/>
                                <Label text="Hours"    />
                            </tab:multiLabels>
                            <tab:template>
                                <Text text="{TimeM2}"/>
                            </tab:template>
                        </tab:Column>      
                        <tab:Column width="11rem" filterProperty="Percent2" headerSpan="2" autoResizable="true" hAlign="Center" >  
                            <tab:multiLabels>
