<mvc:View controllerName="inetum.ssm.alocationssc.controller.Matriz"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:f="sap.f"
    xmlns:tab="sap.ui.table"
    xmlns:core="sap.ui.core">

    <f:DynamicPage id="idDynamicPage" preserveHeaderStateOnScroll="true"  showFooter="true" fitContent="true" title="{i18n>matrizSSC}">

        <!-- DynamicPage Header -->
		<f:header>          
		    <f:DynamicPageHeader id="idDynamicPageHeader"  >    
         
                <fb:FilterBar id="idFilterBar" persistencyKey="myPersKey" useToolbar="false" search="onFilterBarSearch">
                    <fb:filterGroupItems>
                        <fb:FilterGroupItem id="idFilterGroupItem" name="Year" label="{i18n>year}" groupName="Group1" visibleInFilterBar="true">
                            <fb:control>
                                <ComboBox id="idYearListSetComboBox"
                                    name="Year"
                                    items="{
                                        path: '/Year_ListSet',
                                        templateShareable: true
                                    }"                                   
                                >
                                    <core:Item id="idYearItem" key="{Year}" text="{Year}"/>
                                </ComboBox>
                            </fb:control>
                        </fb:FilterGroupItem> 
                        <fb:FilterGroupItem id="idMonthFilterGroupItem" name="Month" label="{i18n>month}"  groupName="Group2" visibleInFilterBar="true">
                            <fb:control>
                                <MultiComboBox id="idMonthtListSetMultiComboBox" name="{i18n>month}">
                                    <core:Item id="idJanuaryItem" key="01" text="{i18n>january}"/>
                                    <core:Item id="idFebruaryItem" key="02" text="{i18n>february}"/>
                                    <core:Item id="idMarchItem" key="03" text="{i18n>march}"/>
                                    <core:Item id="idAprilItem" key="04" text="{i18n>april}"/>
                                    <core:Item id="idMayItem" key="05" text="{i18n>may}"/>
                                    <core:Item id="idJuneItem" key="06" text="{i18n>june}"/>
                                    <core:Item id="idJulyItem" key="07" text="{i18n>july}"/>
                                    <core:Item id="idAugustItem" key="08" text="{i18n>august}"/>
                                    <core:Item id="idSeptemberItem" key="09" text="{i18n>september}"/>
                                    <core:Item id="idOctoberItem" key="10" text="{i18n>october}"/>
                                    <core:Item id="idNovemberItem" key="11" text="{i18n>november}"/>
                                    <core:Item id="idDecemberItem" key="12" text="{i18n>december}"/>
                                </MultiComboBox>
                            </fb:control>
                        </fb:FilterGroupItem>                               
                        <fb:FilterGroupItem id="id2FilterGroupItem" name="Consultant" label="{i18n>consultant}" groupName="Group2" visibleInFilterBar="true">
                            <fb:control>
                                <MultiComboBox id="idConsultantListSetMultiComboBox"
                                    name="{i18n>consultant}"
                                    items="{
                                        path: '/Consultant_ListSet',
                                        templateShareable: true
                                    }" >
                                    <core:Item id="idConsultantNameItem" key="{Consultant}" text="{ConsultantName}"/>
                                </MultiComboBox>
                            </fb:control>
                        </fb:FilterGroupItem>            
                    </fb:filterGroupItems>    
                </fb:FilterBar>
            </f:DynamicPageHeader> 
        </f:header>

        <f:content>                
            <tab:Table id="idTable" rows="{ path: '/AllocationMatrizSet', events: {dataReceived: '.onDataReceive'} }" threshold="100000000"  visibleRowCount="6"  enableColumnFreeze="true" alternateRowColors="true" selectionMode="None" >                                    
                <tab:columns>
                    <tab:Column id="idColumn" width="11rem" filterProperty="ConsultantName" >
                        <Title id="idConsultantTitle" text="{i18n>consultant}" textAlign="Center" width="100%" />
                        <tab:template>
                            <Text id="idConsultantNameText" text="{ConsultantName}"/>
                        </tab:template>
                    </tab:Column>
                    <!-- Janeiro -->
                    <tab:Column id="id1Column" width="230px"  >
                        <Title id="id1Title" textAlign="Center" width="100%" />
                        <tab:template>                            
                            <VBox class="sapUiSmallMarginTopBottom">
                                <HBox>
                                    <Label text="{i18n>planeado}" width="90px" design="Bold" class="sapUiSmallMarginEnd"/>
                                    <Text text="{PlanTimeM1}" width="41.8px"  />
                                    <Text text="H" width="10px" class="sapUiSmallMarginEnd"/>
                                    <Text text="{PlanPercentM1}%" width="40px"/>
                                </HBox>
                                <HBox>
                                    <Text  text="{i18n>real}" width="90px" class="sapUiSmallMarginEnd"/>
                                    <Text text="{RealTimeM1}" width="41.8px"  />
                                    <Text text="H" width="10px" class="sapUiSmallMarginEnd"/>
                                    <Text text="{RealPercentM1}%" width="40px" />
                                </HBox>
                                <HBox>
                                    <Text  text="{i18n>va}" width="90px" class="sapUiSmallMarginEnd"/>
                                    <Text text="{VaabTimeM1}" width="41.8px"  />
                                    <Text text="H" width="10px" class="sapUiSmallMarginEnd"/>
                                    <Text text="{VaabPercentM1}%" width="40px" />
                                </HBox>
                                <HBox>
                                    <Text text="{i18n>nonAllocated}" width="90px" class="sapUiSmallMarginEnd"/>
                                    <ObjectStatus text="{NonalTimeM1}"  class='objstatus' state="{NonalStateM1}" inverted="{NonalCheckM1}"/> 
                                    <Text text="H" width="10px"  class="sapUiSmallMarginEnd" />
                                    <Text text="{NonalPercentM1}%" width="90px"  /> 
                                </HBox>
                                <HBox>
                                    <Label text="{i18n>totalReal}" width="90px" design="Bold" class="sapUiSmallMarginEnd"/>
                                    <Text text="{TotrealTimeM1}" width="41.8px" />
                                    <Text text="H" width="10px" class="sapUiSmallMarginEnd"/>
                                    <Text text="{TotrealPercentM1}%" width="40px" />
                                </HBox>
                                <HBox>
                                    <Label text="{i18n>balance}" width="90px" design="Bold" class="sapUiSmallMarginEnd"/> 
                                    <Text text="{BalanceTimeM1}" width="41.8px" />
                                    <Text text="H" class="sapUiSmallMarginEnd"/>
                                    <ObjectStatus text="{BalancePercentM1}%"  class='objstatus' state="{BalanceStateM1}" inverted="{BalanceCheckM1}"/>  
