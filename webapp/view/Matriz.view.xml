    <f:DynamicPage id="idDynamicPage" preserveHeaderStateOnScroll="true"  showFooter="true" fitContent="true">

    	<!-- DynamicPage Title -->
        <f:title>
            <f:DynamicPageTitle >
                <f:heading>
                    <Title text="{i18n>matrizSSC}"/>
                </f:heading>
 
                <f:content>
                    <OverflowToolbar>
 
                    </OverflowToolbar>
                </f:content>
                <f:actions>

                </f:actions>
            </f:DynamicPageTitle>
        </f:title>

        <!-- DynamicPage Header -->
		<f:header>          
		    <f:DynamicPageHeader id="idDynamicPageHeader"  >                
                <fb:FilterBar id="idFilterBar" persistencyKey="myPersKey" useToolbar="false" search=".onSearch" filterChange=".onFilterChange" afterVariantLoad=".onAfterVariantLoad">
                    <fb:filterGroupItems>
                        <fb:FilterGroupItem name="Year" label="{i18n>year}" groupName="Group1" visibleInFilterBar="true">
                            <fb:control>
                                <ComboBox
                                    name="Year"
                                    selectionChange=".onSelectionChange"
                                    items="{
                                        path: '/Year_ListSet',
                                        templateShareable: true
                                    }"
                                    selectedKey="2024"
                                >
                                    <core:Item key="{Year}" text="{Year}"/>
                                </ComboBox>
                            </fb:control>
                        </fb:FilterGroupItem>                        
                        <fb:FilterGroupItem name="Consultant" label="{i18n>consultant}" groupName="Group2" visibleInFilterBar="true">
                            <fb:control>
                                <MultiComboBox
                                    name="{i18n>consultant}"
                                    selectionChange=".onSelectionChange"
                                    items="{
                                        path: '/Consultant_ListSet',
                                        templateShareable: true
                                    }"
                                >
                                    <core:Item key="{Consultant}" text="{ConsultantName}"/>
                                </MultiComboBox>
                            </fb:control>
                        </fb:FilterGroupItem>
                    
                    </fb:filterGroupItems>    
                </fb:FilterBar>
            </f:DynamicPageHeader>
        </f:header>

        <f:content>                
            <tab:Table id="idTable" rows="{ path: '/AllocationMatrizSet', events: {dataReceived: '.onDataReceive'} }" threshold="100000000"  visibleRowCount="20"  enableColumnFreeze="true" alternateRowColors="true" selectionMode="None" >                                    
                <tab:columns>
                    <tab:Column id="idColumn" width="11rem" filterProperty="ConsultantName">
